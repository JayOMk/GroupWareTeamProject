<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.green.teamproject_groupware.dao.IMemDao">
    
    <select id="loginYn" resultType="com.green.teamproject_groupware.dto.MemDto">
        select mem_pwd from mvc_member where mem_uid=#{param1}
    </select>
    
    <insert id="write">
    	insert into mvc_member values(#{param1},#{param2},#{param3})
    </insert>

</mapper>